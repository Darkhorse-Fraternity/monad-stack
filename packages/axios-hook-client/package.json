{
  "name": "@monad-stack/axios-hook-client",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darkhorse-Fraternity/monad-stack.git"
  },
  "scripts": {
    "build": "tsup ./index.ts --format esm,cjs --dts --external  @tanstack/react-query",
    "dev": "tsup ./index.ts --format esm,cjs --watch --dts --external  @tanstack/react-query",
    "lint": "TIMING=1 eslint *.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "@tanstack/react-query": "^4.14.5"
  },
  "devDependencies": {
    "eslint-config-monad": "workspace:*",
    "@monad-stack/tsconfig": "workspace:*",
    "typescript": "^4.9.3",
    "tsup": "^6.4.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "@tanstack/react-query": "^4.14.5"
  }
}
